<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
        <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
        <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
        <link href="{{ asset('static//css/app.css') }}" rel="stylesheet" type="text/css"/>
        <link href="{{ asset('static//css/bootstrap.css') }}" rel="stylesheet" type="text/css"/>
        <script src="{{ asset('static//js/jssor.slider.debug.js') }}"></script>
        <script src="{{ asset('static//js/jssor.slider.mini.js') }}"></script>
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}


            <div class="col-lg-12 site-header">




                <div class="col-lg-8 site-header shcenter">

                    <nav class="navbar navbar-inverse navbar-fixed-top">

                        <a class="navbar-brand" href="#">Anasayfa</a>

                        <form method="get" action="/search" id="search">


                            <input name="q" type="text" size="40" placeholder="Search"  />
                            <img class="search-img" src="{{ asset('static//img/icon-search-dark.png') }}">
                        </form>
                    </nav>
                </div></div>

        {% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
